
{
  "expo": {
    "name": "Doula Connect",
    "slug": "doula-connect",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/853bfc62-f4e9-4600-9caa-9fd442be4362.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/853bfc62-f4e9-4600-9caa-9fd442be4362.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFF5F0"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.doulaconnect.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/853bfc62-f4e9-4600-9caa-9fd442be4362.png",
        "backgroundColor": "#FFF5F0"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.doulaconnect.app"
    },
    "web": {
      "favicon": "./assets/images/853bfc62-f4e9-4600-9caa-9fd442be4362.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-web-browser"
    ],
    "scheme": "doulaconnect",
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {}
    }
  }
}
