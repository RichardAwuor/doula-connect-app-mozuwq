
{
  "expo": {
    "name": "Doula Connect",
    "slug": "Doula Connect",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/2c7e0498-b523-4877-88ba-eae9afdb3794.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/2c7e0498-b523-4877-88ba-eae9afdb3794.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFF5F0"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.doulaconnect.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/2c7e0498-b523-4877-88ba-eae9afdb3794.png",
        "backgroundColor": "#FFF5F0"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.doulaconnect.app"
    },
    "web": {
      "favicon": "./assets/images/2c7e0498-b523-4877-88ba-eae9afdb3794.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-web-browser"
    ],
    "scheme": "doulaconnect",
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {}
    }
  },
  "scheme": "Doula Connect"
}
